            "field": "replyObj.id",
            "description": "<p>唯一序列号</p>"
          },
          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "replyObj.mobile",
            "description": "<p>回复短信的手机号</p>"
          },
          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "replyObj.reply_time",
            "description": "<p>回复短信的时间</p>"
