          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "replyObj.text",
            "description": "<p>回复的短信内容</p>"
          },
          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "replyObj.extend",
